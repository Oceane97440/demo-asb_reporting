<%- include("../layouts/admin/head.ejs") -%>

<div id="card-forecast" class="card" style="margin-top: 30px;">
    <div class="card-header">
        <h1 class="card-title">
            Données du Reporting <%=table.Date_rapport%>
        </h1>
    </div>

    <div class="card-body">
       

        <ul>
            <li><strong>Total Impressions:<%=table.TotalImpressions%></strong></li>
            <li>Total Clics:<%=table.TotalCliks%></li>
            <li>Taux de clics:<%=table.CTR%>%</li>
            <li>Visiteur Unique:<%=table.Total_VU%></li>
            <li>Répétition:<%=table.Repetition%></li>



        </ul>

        <hr>
        <br>
        <h3>Tableau récapitulatif des données du Reporting</h3>
        <br>

        <table class="table">
            <thead class="text-left thead-dark">
                <tr>

                    <th scope="col">Format</th>
                    <th scope="col">Total impression</th>
                    <th scope="col">Total Clics</th>
                    <th scope="col">CTR(%)</th>
                   


                </tr>
            </thead>
            
            <tbody>
           
                  
                <tr>
                    <td>HABILLAGE</td>
                    <td><%=table.sommeHabillageImpression%></td>
                    <td><%=table.sommeHabillageClicks%></td>
                    <td><%=table.CTR_habillage%>%</td>
                </tr>
                <tr>
                 
                    <td>INTERSTITIEL</td>
                    <td><%=table.sommeInterstitielImpression%></td>
                    <td><%=table.sommeInterstitielClicks%></td>
                    <td><%=table.CTR_interstitiel%>%</td>
                   
                </tr>
                <tr>
                 
                    <td>MASTHEAD</td>
                    <td><%=table.sommeMastheadImpression%></td>
                    <td><%=table.sommeMastheadClicks%></td>
                    <td><%=table.CTR_masthead%>%</td>
                   
                </tr>
                <tr>
                 
                    <td>GRAND ANGLE</td>
                    <td><%=table.sommeGrand_AngleImpression%></td>
                    <td><%=table.sommeGrand_AngleClicks%></td>
                    <td><%=table.CTR_grand_angle%>%</td>
                   
                </tr>
                <tr>
                 
                    <td>NATIVE</td>
                    <td><%=table.sommeNativeImpression%></td>
                    <td><%=table.sommeNativeClicks%></td>
                    <td><%=table.CTR_native%>%</td>
                   
                </tr>

           
            </tbody>
          

        </table>

        <table class="table">
            <thead class="text-left thead-dark">
                <tr>

                    <th scope="col">CampaignName</th>
                    <th scope="col">InsertionName</th>
                    <th scope="col">FormatName</th>
                    <th scope="col">SiteName</th>
                    <th scope="col">Impressions</th>
                    <th scope="col">Clicks</th>
                    <th scope="col">ClickRate(%)</th>



                </tr>
            </thead>
            
            <tbody>
                <% for (let i=1; i < table.CampaignName.length; i++) { %>
                  
                <tr>
                 

                    <td><%=table.CampaignName[i]%></td>
                    <td><%=table.InsertionName[i]%></td>
                    <td><%=table.FormatName[i]%></td>
                    <td><%=table.SiteName[i]%></td>
                    <td><%=table.Impressions[i]%></td>
                    <td><%=table.Array_Clicks[i]%></td>
                    <td><%=table.ClickRate[i]%></td>

                </tr>

                <% } %>

            </tbody>
          

        </table>

    </div>

</div>


       
    <%- include("../layouts/admin/footer.ejs") -%>