<div class="table-responsive">
    <table class="table table-striped table_sort">
        <thead>
            <tr>
                <th class="advertiser_name">Annonceur</th>
                <th class="campaign_name">Nom</th>
                <th >Impressions</th>
                <th >Clics</th>
             
            </tr>
        </thead>
        <tbody>
            <% Object.keys(campaigns).forEach(key =>  { %>
               
            <tr>
               
                   
                <td class="advertiser_name">
                    <%= campaigns[key].advertiser_name %>
                </td>
                <td class="campaign_name">
                    <a href="/manager/campaigns/ <%= campaigns[key].campaign_id %>"
                        title="<%= campaigns[key].campaign_name %>"><%= campaigns[key].campaign_name %>
                        <br />
                        <span class="small">(Du
                            <%= moment(parseInt(campaigns[key].campaign_start_date)).format('DD/MM/YYYY') %>
                            au
                            <%= moment(parseInt(campaigns[key].campaign_end_date)).format('DD/MM/YYYY') %>)</span>
                    </a>
                </td>
          
                
               <td><%= campaigns[key].impressions %></td>
               <td><%= campaigns[key].clicks %></td>

            </tr>
            <% }) %>
        </tbody>
    </table>
</div>