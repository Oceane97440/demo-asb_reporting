<div class="table-responsive">
    <table class="table table-striped table_sort">
        <thead>
            <tr>
                <th>Annonceur</th>
                <th>Nom de la campagne</th>
                <th>Date de d√©but</th>
                <th>Date de fin</th>
                <th>Impressions</th>
                <th>Clics</th>

            </tr>
        </thead>
        <tbody>
            <% Object.keys(campaigns).forEach(key=> { %>

                <tr>


                    <td>
                        <a href="/manager/advertisers/<%= campaigns[key].advertiser_id %>">
                            <%= campaigns[key].advertiser_name %>
                        </a>
                    </td>
                    <td>
                        <a href="/manager/campaigns/ <%= campaigns[key].campaign_id %>">
                            <%= campaigns[key].campaign_name %>
                        </a>
                    </td>

                    <td>
                        <%= moment(campaigns[key].campaign_start_date).format('DD/MM/YYYY') %>
                    </td>
                    <td>
                        <%= moment(campaigns[key].campaign_end_date).format('DD/MM/YYYY') %>
                    </td>

                    <td>
                        <%= utilities.numStr(campaigns[key].impressions) %>
                    </td>
                    <td>
                        <%= utilities.numStr(campaigns[key].clicks) %>
                    </td>

                </tr>
                <% }) %>
        </tbody>
    </table>
</div>