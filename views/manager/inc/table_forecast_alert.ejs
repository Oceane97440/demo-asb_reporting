<div class="table-responsive">

    <table class="table table-striped table_sort">
        <thead>
            <tr>
                <th>#</th>
                <th>Campagne</th>
                <th>Insertion</th>
                <th>Date de début</th>
                <th>Date de fin</th>
                <th>% délivrer</th>
            </tr>
        </thead>
        <tbody>
            <% Object.keys(campaigns).forEach(i=> { %>


                <tr>
                    <td>
                        <%= campaigns[i].campaign_id %>
                    </td>

                    <td>
                        <a href="https://manage.smartadserver.com/gestion/smartprog2.asp?CampagneID=<%= campaigns[i].campaign_id %>"
                            target="_blank" ><%= campaigns[i].campaign_name %></a>
                    </td>
                    <td>
                        <a href="https://manage.smartadserver.com/gestion/smartprog3.asp?InsertionID=<%= campaigns[i].insertion_id %>"
                            target="_blank" ><%= campaigns[i].insertion_name %></a>
                    </td>
                    <td>
                        <%= moment(campaigns[i].campaign_start_date).format('DD/MM/YYYY') %>
                    </td>

                    <td>
                        <%= moment(campaigns[i].campaign_end_date).format('DD/MM/YYYY') %>
                    </td>
                    <td>
                        <%= campaigns[i].delivered_percentage %>%
                    </td>


                </tr>
                <% });%>
        </tbody>
    </table>
</div>